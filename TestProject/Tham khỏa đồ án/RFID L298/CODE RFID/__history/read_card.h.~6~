#define _read_card_h_
#include <math.h>//chen thu vien toan
#use delay(clock=20000000)

unsigned int8 *card_id();

unsigned int8 *card_id(){
   unsigned int8 UID[5];
   
   if (MFRC522_isCard (&TagType)) //Check any card
       {             
        if(MFRC522_ReadCardSerial (&UID)) 
        {
            return UID;                    
        }        
        MFRC522_Halt() ; 
    }
    else return 0;
    
}
